<div>
  <center>
    <table class="roundedCorners" id="subjectNew">
      <thead>
        <tr>
          <th><%= link_to "Add new Subject", new_subject_path %></th>
        </tr>
      </thead>
    </table>
    <table class="roundedCorners" id="subjectlist">
      <thead>
        <tr>
          <th>Branch</th>
          <th>Semester</th>
          <th>Subject</th>
          <th></th>
          <th></th>
        </tr>
      </thead>

      <% @subjects.each do |subject| %>
        <tr>
          <td><%= subject.branch.name %></td>
          <td><%= subject.semester.sem %></td>
          <td><%= subject.name %></td>
          <td><%= link_to("Edit", edit_subject_path(subject), data: { confirm: "Are you sure?" }) %></td>
          <td><%= link_to("Delete", subject_path(subject), method: :delete, data: { confirm: "Are you sure?" }) %></td>
        </tr>
      <% end %>
    </table>
  </center>
</div>
